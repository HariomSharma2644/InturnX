{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "client/dist",
  "installCommand": "echo 'Skipping root install - using client and server package.json'",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}

